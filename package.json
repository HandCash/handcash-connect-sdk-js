{
	"name": "@handcash/handcash-connect",
	"version": "0.5.0",
	"description": "HandCash Connect SDK",
	"main": "src/index.js",
	"type": "module",
	"types": "types/index.d.ts",
	"scripts": {
		"test_old": "NODE_ENV=test mocha \"test/**/*.test.js\" --timeout 10000 --file test/setup.js --exit",
		"start": "node src/app/app.js",
		"lint_src": "eslint --ext .js src/",
		"lint_test": "eslint --ext .test.js test/",
		"coverage_old": "NODE_ENV=test nyc mocha \"test/**/*.test.js\" --timeout 10000 --exit --file test/setup.js",
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "prettier --ignore-path .gitignore --check . && eslint . --fix",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"keywords": [],
	"author": "HandCash Labs S.L",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.27.2",
		"bsv": "^1.5.4",
		"dotenv": "^8.1.0",
		"dotenv-flow": "^3.1.0",
		"run-sdk": "^0.6.41",
		"underscore": "^1.9.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"@vitest/coverage-c8": "^0.23.4",
		"eslint": "^8.23.1",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.7.1",
		"typescript": "^4.8.3",
		"vite": "^3.1.3",
		"vite-plugin-dts": "^1.6.4",
		"vitest": "^0.23.4"
	},
	"directories": {
		"test": "test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HandCash/handcash-connect-sdk-js.git"
	},
	"bugs": {
		"url": "https://github.com/HandCash/handcash-connect-sdk-js/issues"
	},
	"homepage": "https://github.com/HandCash/handcash-connect-sdk-js#readme"
}
